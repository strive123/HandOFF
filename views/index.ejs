        <!-- The Modal of signin -->
        <div id="myModal-sign-in" class="modal">
            <span class="close">&times;</span>
            <!-- Modal content -->
            <div class="modal-content">
                <!-- Header -->
                <div class="modal-header">
                    <div class="row">
                        <div class="text">
                            <h1><strong>HandOFF</strong></h1>
                            <div class="description">
                                <p> Connect Your Apps and Automate Workflows</p>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Body -->
                <div class="modal-body">
                    <div class="row">
                        <div class="form-box">
                            <div class="form-top">
                                <div class="form-top-left">
                                    <h3><strong>Login</strong></h3>
                                    <p>Enter your username and password to log on:</p>
                                </div>

                                <div class="form-top-right">
                                    <i class="fa fa-lock"></i>
                                </div>
                            </div>
                            <% if (typeof messagesLogin != 'undefined') { %>
                            <div class="alert alert-danger"><%= messagesLogin %></div>
                            <% } %>
                            <div class="form-bottom">
                                <form role="form" action="/login" method="post" class="login-form">
                                    <div class="form-group">
                                        <label class="sr-only" for="form-username">Username</label>
                                        <input type="text" name="username" placeholder="Username or Email" class="form-username form-control" id="form-username">
                                    </div>
                                    <div class="form-group">
                                        <label class="sr-only" for="form-password">Password</label>
                                        <input type="password" name="password" placeholder="Your Password" class="form-password form-control" id="form-password">
                                    </div>
                                    <button type="submit" class="btn">Sign in</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- footer -->
                <div class="modal-footer">
                    <div class="row">
                        <div class="social-login">
                            <h3>...or login with:</h3>
                            <div class="social-login-buttons">
                                <a class="btn btn-link-2" href="/auth/facebook">
                                    <i class="fa fa-facebook"></i> Facebook
                                </a>
                                <a class="btn btn-link-2" href="/auth/google">
                                    <i class="fa fa-google-plus"></i> Google Plus
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Modal sign up -->
        <div id="myModal-sign-up" class="modal">
            <span class="close">&times;</span>
            <!-- Modal content -->
            <div class="modal-content">
                <!-- Header -->
                <div class="modal-header">
                    <div class="row">
                        <div class="text">
                            <h1><strong>HandOFF</strong></h1>
                            <div class="description">
                                <p> Connect Your Apps and Automate Workflows
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Body -->
                <div class="modal-body">
                    <div class="row">
                        <div class="form-box">
                            <div class="form-top">
                                <div class="form-top-left">
                                    <h3><strong>Sign up now</strong></h3>
                                    <p>Enter your username and password to sign up:</p>
                                </div>
                                <div class="form-top-right">
                                    <i class="fa fa-lock"></i>
                                </div>
                            </div>
                            <% if (typeof messagesSignUp != 'undefined') { %>
                            <div class="alert alert-danger"><%= messagesSignUp %></div>
                            <% } %>

                            <div class="form-bottom">
                                <form role="form" action="/signup" method="post" class="login-form">
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="sr-only" for="form-username">Name</label>
                                                <input type="text" name="username" placeholder="User Name..." class="form-username form-control" id="form-username">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="sr-only" for="form-username">Username</label>
                                                <input type="text" name="email" placeholder="Your Email..." class="form-username form-control" id="form-username">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="sr-only" for="form-password">Password</label>
                                        <input type="password" id="password" name="password" placeholder="Password..." class="form-password form-control" id="form-password" required>
                                    </div>
                                    <div class="form-group">
                                        <label class="sr-only" for="form-password">Password</label>
                                        <input type="password" id="confirm-password" name="form-confirm-password" placeholder="Confirm Password..." class="form-password form-control" id="form-password" required>
                                    </div>
                                    <button type="submit" class="btn">Sign up</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
